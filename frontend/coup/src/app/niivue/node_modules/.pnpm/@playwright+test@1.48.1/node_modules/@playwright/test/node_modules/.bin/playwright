#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/legato/Downloads/viewer 2/niivue/node_modules/.pnpm/playwright@1.48.1/node_modules/playwright/node_modules:/Users/legato/Downloads/viewer 2/niivue/node_modules/.pnpm/playwright@1.48.1/node_modules:/Users/legato/Downloads/viewer 2/niivue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/legato/Downloads/viewer 2/niivue/node_modules/.pnpm/playwright@1.48.1/node_modules/playwright/node_modules:/Users/legato/Downloads/viewer 2/niivue/node_modules/.pnpm/playwright@1.48.1/node_modules:/Users/legato/Downloads/viewer 2/niivue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../playwright@1.48.1/node_modules/playwright/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../playwright@1.48.1/node_modules/playwright/cli.js" "$@"
fi
